# Copy this file to .env and fill values or set environment variables directly.
# Path to the Firebase service account JSON used to verify Firebase ID tokens
FIREBASE_SERVICE_ACCOUNT=/path/to/firebase-service-account.json
# Path to the Google service account JSON used to download Drive files
GOOGLE_SERVICE_ACCOUNT_FILE=/path/to/google-service-account.json
# Optional: host and port for uvicorn
HOST=0.0.0.0
PORT=8000
